let test = ['/Tools', '/Tools/SysBtn_Back.png', '/Tools/SysBtn_Back.png'];

const showFolder = (data, items) => {
    let show = false;

    let arr = [];

    const fn = (data) => {
        data.forEach((item) => {
            if (item.type === 'FOLDER') {
                fn(item.children);
            } else {
                arr.push(item.name);
            }
        });
    };
    fn(data);

    items.forEach((item) => {
        arr.forEach((f) => {
            if (item.includes(f)) {
                show = true;
            }
        });
    });

    return show;
};

console.log(showFolder(dates, test));
